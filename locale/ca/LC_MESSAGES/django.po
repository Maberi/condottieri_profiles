#: templates/notification/friend_joined/full.txt:3
#: templates/notification/friend_joined/notice.html:3
#, python-format
msgid "Your friend %(username)s has joined the game %(slug)s."
msgstr "El teu amic %(username)s s'ha unit a la partida %(slug)s."

#: templates/notification/friend_joined/full.txt:5
msgid "Go to the game"
msgstr "Anar al joc"

#: templates/notification/friend_joined/full.txt:7
#, python-format
msgid ""
"You receive this notice because %(username)s and you are\n"
"friends in Condottieri."
msgstr ""
"Reps aquest avís perquè %(username)s i tu sou \n"
"amics en Condottieri."

#: templates/notification/karma_healed/full.txt:3
msgid ""
"You have recovered enough karma to join a game or create a new\n"
"one."
msgstr ""
"Has recuperat el karma mínim per unir-te a un joc o crear-ne un\n"
"de nou."

#: templates/notification/karma_healed/full.txt:6
msgid ""
"Before joining or creating a game, please, be sure that you\n"
"are really interested in playing it."
msgstr ""
"Abans d'unir-te a un joc o crear-ne un, si us plau, assegurat que\n"
"estàs realment interessat en participar-hi."

#: templates/notification/new_friend/full.txt:3
#, python-format
msgid "%(username)s has added you as a friend in Condottieri."
msgstr "%(username)s t'ha afegit com amic en Condottieri."

#: templates/notification/new_friend/full.txt:5
msgid "See the profile"
msgstr "Veure el perfil"

